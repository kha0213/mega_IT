package com.tj.ex2_parkingSystem;

public class Parking {
	private int into;
	private String carNum;
	private int out;
	private int parkMoney;
	parkMoney = (out-into)*2000;
	
	
	public Parking() {
		
	}
	public Parking(String carNum, int into) {
		this.carNum=carNum;
		this.into=into;		
	}
	public Parking(String carNum, int into,int out) {
		this.carNum=carNum;
		this.into=into;		
		this.out=out;
	}
	
	
	

}

